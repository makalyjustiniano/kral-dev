<?versión xml="1.0" codificación="UTF-8"?>
<odoo>
    <data>
        <record id="custom_stock_wizard_form_view" model="ir.ui.view">
            <field name="name">custom.stock.wizard.form</field>
            <field name="model">custom.stock.wizard</field>
            <field name="arch" type="xml">
                <form string="Cargar Data">
                    <group>
                        <group>
                          <field name="picking_id" string="Número de Documento" readonly="1" />
                          <field name="excel_file" string="Archivo"/> 
                        </group>
                     </group>
                    <footer>
                        <style>
                            .boton_test{
                                background: white;
                                color: black;
                                border: 2px solid black;
                                border-radius: 25px;
                            }
                        </style>
                        <button name="cargar_data" type="object" string="Cargar" class="oe_highlight boton_test" />
                        <button special="cancel" string="Cancelar" class="oe_highlight boton_test" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
